#!/usr/bin/with-contenv bash

ACCESS_CONTROL=${ACCESS_CONTROL:-"0.0.0.0/0 allow"}
ACCESS_CONTROL_OUTGOING_PORT_PERMIT=${ACCESS_CONTROL_OUTGOING_PORT_PERMIT:-"10240-65535"}
BUFFER_SIZE_MSG=${BUFFER_SIZE_MSG:-"8192"}
CACHE_SIZE_EDNS=${CACHE_SIZE_EDNS:-"4096"}
CACHE_SIZE_MSG=${CACHE_SIZE_MSG:-"32m"}
CACHE_SIZE_RRSET=${CACHE_SIZE_RRSET:-"64m"}
CACHE_SLABS_MSG=${CACHE_SLABS_MSG:-"4"}
CACHE_TTL_MAX=${CACHE_TTL_MAX:-"86400"}
CACHE_TTL_MAX_NEGATIVE=${CACHE_TTL_MAX_NEGATIVE:-"10"}
CACHE_TTL_MIN=${CACHE_TTL_MIN:-"300"}
CONFIG_FILE=${CONFIG_FILE:-"unbound.conf"}
CONFIG_PATH=${CONFIG_PATH:-"/config/"}
DENY_ANY=${DENY_ANY:-"TRUE"}
ENABLE_IPV4=${ENABLE_IPV4:-"TRUE"}
ENABLE_IPV6=${ENABLE_IPV6:-"TRUE"}
ENABLE_TCP=${ENABLE_TCP:-"TRUE"}
ENABLE_UDP=${ENABLE_UDP:-"TRUE"}
HARDEN_DNSSEC_STRIPPED=${HARDEN_DNSSEC_STRIPPED:-"TRUE"}
HARDEN_GLUE=${HARDEN_GLUE:-"TRUE"}
HARDEN_LARGE_QUERIES=${HARDEN_LARGE_QUERIES:-"TRUE"}
HARDEN_SHORT_BUFFER_SIZE=${HARDEN_SHORT_BUFFER_SIZE:-"TRUE"}
LISTEN_IP=${LISTEN_IP:-"0.0.0.0"}
LISTEN_PORT=${LISTEN_PORT:-"53"}
LOG_FILE=${LOG_FILE:-"unbound.log"}
LOG_LEVEL=${LOG_LEVEL:-"1"}
LOG_PATH=${LOG_PATH:-"/logs/"}
LOG_QUERIES=${LOG_QUERIES:-"TRUE"}
LOG_REPLIES=${LOG_REPLIES:-"TRUE"}
LOG_TYPE=${LOG_TYPE:-"FILE"}
MINIMAL_RESPONSES=${MINIMAL_RESPONSES:-"FALSE"}
NO_QUERY_LOCALHOST=${NO_QUERY_LOCALHOST:-"FALSE"}
PREFETCH=${PREFETCH:-"TRUE"}
PREFETCH_KEY=${PREFETCH_KEY:-"TRUE"}
ROOT_HINTS=${ROOT_HINTS:-"${CONFIG_PATH}root.hints"}
QNAME_MINIMIZATION=${QNAME_MINIMIZATION:-"TRUE"}
RRSET_ROUNDROBIN=${RRSET_ROUNDROBIN:-"TRUE"}
SETUP_TYPE=${SETUP_TYPE:-"AUTO"}
SO_BUFFER_RECEIVE=${SO_BUFFER_RECEIVE:-""}
SO_BUFFER_SEND=${SO_BUFFER_SEND:-""}
SO_REUSE_PORT=${SO_REUSE_PORT:-"FALSE"}
STATISTICS_CUMULATIVE=${STATISTICS_CUMULATIVE:-"TRUE"}
STATISTICS_EXTENDED=${STATISTICS_EXTENDED:-"TRUE"}
STATISTICS_INTERVAL=${STATISTICS_INTERVAL:-"24300"}
TARGET_FETCH_POLICY=${TARGET_FETCH_POLICY:-"2 1 0 0 0 0"}
THREADS=${THREADS:-"1"}
THREADS_QUERIES_PER=${THREADS_QUERIES_PER:-"1024"}
TRUST_ANCHOR=${TRUST_ANCHOR:-". DS 19036 8 2 49AAC11D7B6F6446702E54A1607371607A1A41855200FD2CE1CDDE32F24E8FB5"}
